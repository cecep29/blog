<h1> Buat Article</h1>
<%= form_for :article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body ,:class => "tinymce" , id: "elm1" %>
  </p>
 
  <p>
    <%= f.submit  :class => 'btn btn-primary' %>
  </p>
<% end %> 
